{
  "kind": "Deployment",
  "apiVersion": "apps/v1",
  "metadata": {
    "name": "blue",
    "labels": {
      "app": "blue"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "matchLabels": {
        "app": "blue"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "blue"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "blue",
            "image": "haymarnhm/capstone-react-app:latest",
            "ports": [
              {
                "name": "http-server",
                "containerPort": 3000
              }
            ]
          }
        ]
      }
    }
  }
}